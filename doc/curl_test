// 가입하기
curl --request POST "http://localhost:3000/api/signup" -H "Content-type:application/json" -d '{"email":"email@gmail.com", "password":"1234"}' -i

// 로그인 토큰
curl --request POST "http://localhost:3000/api/login" -H "Content-type:application/json" -H "Auth:144ref" -d '{"email":"email@gmail.com", "password":"1234"}' -i